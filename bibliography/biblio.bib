  isbn = {0171-8630},
  issn = {01718630},
  keywords = {EU Marine strategy Framework Directive,Ecosystem approach,Functional and response biodiversity,Re-gime shift,Resilience,State space},
  mendeley-groups = {Sorting/Unification folder/Important / relevant,Sorting/Unification folder/Important / relevant/Ecosystem health and ecosystem services},
