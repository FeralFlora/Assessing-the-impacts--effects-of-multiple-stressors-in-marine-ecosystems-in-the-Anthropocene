@article{Calow2003,
  abstract = {What is ecotoxicology for? One important answer to this question is to in-form ecological risk assessment, that is, to provide a basis for assessing whether industrial and agricultural chemicals are likely to have adverse effects on ecosystems and hence provide a foundation for managing them. But ecotoxicology has evolved more from toxicology than ecology, and so there are those who argue that it is too simplistic to do the job effectively. This begs a number of questions that we shall address in this article: What kinds of adverse ecological effects are of concern in ecological risk assessment? How can we anticipate these effects to put into practice appropriate controls? How can we check the effectiveness of these approaches? Extrapolating the results of ecotoxicological tests to likely ecosystem effects undoubtedly involves a number of uncertainties. We shall discuss sources of these uncertainties and suggest ways that they might be addressed. Recent research suggests that there may be some general rules, which should increase our confidence in extrapolating results from simple to more complex ecological systems. However, there re-mains substantial room for improvement.},
  author = {Calow, Peter and Forbes, Valery E.},
  doi = {10.1021/es0324003},
  file = {:C$\backslash$:/Users/Ingmar/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Calow, E. Forbes - Unknown - Does Ecotoxicology Inform Ecological Risk Assessment.pdf:pdf},
  isbn = {0013-936X},
  issn = {0013-936X},
  journal = {Environmental Science {\&} Technology},
  mendeley-groups = {Sorting/Unification folder/New Articles 03/10},
  number = {7},
  pages = {146A--151A},
  title = {{Does Ecotoxicology Inform Ecological Risk Assessment?}},
  url = {http://pubs.acs.org.ep.fjernadgang.kb.dk/doi/pdf/10.1021/es0324003 http://dx.doi.org/10.1021/es0324003},
  volume = {37},
  year = {2003},
}


@article{Segner2014,
  abstract = {Aquatic ecosystems are confronted with multiple stress factors. Current approaches to assess the risk of anthropogenic stressors to aquatic ecosystems are developed for single stressors and determine stressor effects primarily as a function of stressor properties. The cumulative impact of several stressors, however, may differ markedly from the impact of the single stressors and can result in nonlinear effects and ecological surprises. To meet the challenge of diagnosing and predicting multiple stressor impacts, assessment strategies should focus on properties of the biological receptors rather than on stressor properties. This change of paradigm is required because (i) multiple stressors affect multiple biological targets at multiple organizational levels, (ii) biological receptors differ in their sensitivities, vulnerabilities, and response dynamics to the individual stressors, and (iii) biological receptors function as networks, so that actions of stressors at disparate sites within the network can lea...},
  annote = {Initial Reading Period: Ingmar Reading period 3: Tabea Unified Relevant The article emphasizes that the properties of a single stressor might be very different from the cumulative effect of multiple stressors, which may cause nonlinear effects and ecological surprises. From this claim, the articles argues that in order to diagnose and predict the effects of multiple stressors, one should focus on the biological receptors rather than the single stressor properties themselves. This, they argue, is because - Multiple stressors impact multiple biological receptors at multiple organizational levels simultaneously. - Biological receptors have differing responses to the same individual stressors. - Biological receptors are network, which, when affected on various fronts, might suffer cascading effects. - Ingmar Really good concentrating on the whole picture of multiple stressors. - TABEA},
  author = {Segner, H. and Schmitt-Jansen, M. and Sabater, S.},
  doi = {10.1021/es405082t},
  file = {:C$\backslash$:/Users/Ingmar/Documents/Ingmar/{\'{U}}tb{\'{u}}gving/RUC/NIB/Semester 1/Project/Mendeley/RP3/es405082t.pdf:pdf},
  isbn = {0013-936X},
  issn = {15205851},
  journal = {Environmental Science and Technology},
  mendeley-groups = {Sorting/Unification folder/Important / relevant,Sorting/Unification folder/Important / relevant/Main reading / Multiple stressor assessment methods},
  month = {jul},
  number = {14},
  pages = {7690--7696},
  pmid = {24905720},
  publisher = {American Chemical Society},
  title = {{Assessing the impact of multiple stressors on aquatic biota: The receptor's side matters}},
  url = {http://pubs.acs.org/doi/abs/10.1021/es405082t},
  volume = {48},
  year = {2014},
}


@article{Micheli2013,
  abstract = {Management of marine ecosystems requires spatial information on current impacts. In several marine regions, including the Mediterranean and Black Sea, legal mandates and agreements to implement ecosystem-based management and spatial plans provide new opportunities to balance uses and protection of marine ecosystems. Analyses of the intensity and distribution of cumulative impacts of human activities directly connected to the ecological goals of these policy efforts are critically needed. Quantification and mapping of the cumulative impact of 22 drivers to 17 marine ecosystems reveals that 20{\%} of the entire basin and 60-99{\%} of the territorial waters of EU member states are heavily impacted, with high human impact occurring in all ecoregions and territorial waters. Less than 1{\%} of these regions are relatively unaffected. This high impact results from multiple drivers, rather than one individual use or stressor, with climatic drivers (increasing temperature and UV, and acidification), demersal fishing, ship traffic, and, in coastal areas, pollution from land accounting for a majority of cumulative impacts. These results show that coordinated management of key areas and activities could significantly improve the condition of these marine ecosystems.},
  author = {Micheli, Fiorenza and Halpern, Benjamin S. and Walbridge, Shaun and Ciriaco, Saul and Ferretti, Francesco and Fraschetti, Simonetta and Lewison, Rebecca and Nykjaer, Leo and Rosenberg, Andrew A.},
  doi = {10.1371/journal.pone.0079889},
  file = {:C$\backslash$:/Users/PC/Desktop/stressors/important aricles(stressors)/multiple stressors/Cumulative Human Impacts on Mediterranean and Black sea.PDF:PDF},
  isbn = {1932-6203},
  issn = {19326203},
  journal = {PLoS ONE},
  mendeley-groups = {Sorting/Unification folder/Important / relevant/Main reading / Multiple stressor assessment methods/new articles(writing period)},
  number = {12},
  pmid = {24324585},
  title = {{Cumulative human impacts on Mediterranean and Black Sea marine ecosystems: Assessing current pressures and opportunities}},
  volume = {8},
  year = {2013},
}


@inproceedings{Costanza1999,
  abstract = {Rapid deterioration of the world's major ecosystems has intensified the need for effective environmental monitoring and the development of operational indicators of ecosystem health. Ecosystem health represents a desired endpoint of environmental management, but it requires adaptive, ongoing definition and assessment. We propose that a healthy ecosystem is one that is sustainable - that is, it has the ability to maintain its structure (organization) and function (vigor) over time in the face of external stress (resilience). Various methods to quantify these three ecosystem attributes (vigor, organization, and resilience) are discussed. These attributes are then folded into a comprehensive assessment of ecosystem health. A network analysis based ecosystem health assessment is developed and tested using trophic exchange networks representing several different aquatic ecosystems. Results indicate the potential of such an ecosystem health assessment for evaluating the relative health of similar ecosystems, and quantifying the effects of natural or anthropogenic stress on the health of a particular ecosystem over time.},
