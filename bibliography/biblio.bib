  journal = {JOURNAL OF AQUATIC ECOSYSTEM HEALTH},
  keywords = {diagnosis,early warning,ecosystem,health,stress},
  mendeley-groups = {Sorting/Unification folder/Important / relevant,Sorting/Unification folder/Important / relevant/Ecosystem health and ecosystem services},
  number = {1},
